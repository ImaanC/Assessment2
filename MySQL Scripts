CREATE SCHEMA `college` ;


CREATE TABLE `college`.`student` (
  `ID` INT NOT NULL,
  `Firstname` VARCHAR(45) NULL,
  `Surname` VARCHAR(45) NULL,
  `Algebra` INT NULL,
  `Calculus` INT NULL,
  `Programming` INT NULL,
  `Databases` INT NULL,
  PRIMARY KEY (`ID`));


