CREATE TABLE `college`.`grades` (
  `ID` INT NOT NULL,
  `Firstname` VARCHAR(45) NOT NULL,
  `Surname` VARCHAR(45) NOT NULL,
  `Average` INT NOT NULL,
  `Grade` INT NOT NULL,
  PRIMARY KEY (`ID`));
  
  ALTER TABLE `college`.`grades` 
CHANGE COLUMN `Grade` `Grade` VARCHAR(5) NOT NULL ;

